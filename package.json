{
  "name": "react-bootstrap-datetimepicker",
  "version": "0.0.21",
  "description": "A bootstrap datetime picker component for React.js",
  "homepage": "http://dev.quri.com/react-bootstrap-datetimepicker/",
  "repository": {
    "type": "git",
    "url": "http://github.com/quri/react-bootstrap-datetimepicker"
  },
  "main": "./dist/react-bootstrap-datetimepicker.js",
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production webpack",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "react-component",
    "bootstrap",
    "datetimepicker",
    "datetime"
  ],
  "author": "Lo√Øc CHOLLIER <loic@chollier.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-bootstrap": "^0.16.1",
    "moment": "^2.8.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "core-js/.*",
      "<rootDir>/node_modules/react",
      "babel",
      "<rootDir>/node_modules/babel"
    ],
    "setupEnvScriptFile": "<rootDir>/jestEnvironment.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathDirs": [
      "src/"
    ]
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.20.2",
    "eslint": "^1.7.2",
    "eslint-plugin-jasmine": "^1.5.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.2",
    "jest-cli": "^0.5.10",
    "jsx-loader": "^0.13.2",
    "moment": "^2.10.6",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "classnames": "^2.2.0",
    "lodash": "^3.10.1",
    "react-css-modules": "^3.6.0"
  }
}
