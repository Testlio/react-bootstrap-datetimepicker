{
  "name": "@testlio/react-bootstrap-datetimepicker",
  "version": "0.1.1",
  "description": "A bootstrap datetime picker component for React.js",
  "homepage": "http://dev.quri.com/react-bootstrap-datetimepicker/",
  "repository": {
    "type": "git",
    "url": "http://github.com/quri/react-bootstrap-datetimepicker"
  },
  "main": "./dist/react-bootstrap-datetimepicker.js",
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production webpack",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "react-component",
    "bootstrap",
    "datetimepicker",
    "datetime"
  ],
  "author": "Lo√Øc CHOLLIER <loic@chollier.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "moment": "^2.10.6",
    "react-bootstrap": "^0.28.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "core-js/.*",
      "<rootDir>/node_modules/react",
      "babel",
      "<rootDir>/node_modules/babel"
    ],
    "setupEnvScriptFile": "<rootDir>/jestEnvironment.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testPathDirs": [
      "src/"
    ]
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.7.2",
    "eslint-plugin-jasmine": "^1.5.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-webpack-plugin": "^1.6.2",
    "jest-cli": "^0.7.1",
    "jsx-loader": "^0.13.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel-runtime": "^6.1.18",
    "classnames": "^2.2.0",
    "moment": "^2.10.6",
    "lodash": "^3.10.1",
    "react-css-modules": "^3.6.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-bootstrap": "^0.28.1"
  }
}
